      <if test="classid != null" >
        CLASSID = #{classid,jdbcType=DECIMAL},
      </if>
    </set>
    where ID = #{id,jdbcType=DECIMAL}
  </update>
  <update id="updateByPrimaryKey" parameterType="ls.model.Student" >
    update STUDENT
